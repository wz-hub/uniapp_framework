{
	
	"easycom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		
		
		{
			"path": "pages/index/index",
			"aliasPath":"/",
			"name":"index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
		    "path" : "pages/login/login",
			"name":"login",
		    "style": {
		    	"navigationBarTitleText": "注册登录",
		    	"navigationBarBackgroundColor": "#19be6b"
		    }
			
		}
		
        ,{
            "path" : "pages/scancode/scancode",
			"name":"scancode",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "扫码",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/me/me",
			"name":"me",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "我的",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
	        "color": "#7A7E83",
	        "selectedColor": "#3cc51f",
	        "borderStyle": "black",
	        "backgroundColor": "#ffffff",
	        "height": "50px",
	        "fontSize": "10px",
	        "iconWidth": "24px",
	        "spacing": "3px",
	        "list": [{
	            "pagePath": "pages/index/index",
	            "iconPath": "static/index_ht.png",
	            "selectedIconPath": "static/index.png",
	            "text": "订单"
	        }, {
	            "pagePath": "pages/me/me",
	            "iconPath": "static/me_ht.png",
	            "selectedIconPath": "static/me.png",
	            "text": "扫码"
	        }, {
	            "pagePath": "pages/scancode/scancode",
	            "iconPath": "static/code_ht.png",
	            "selectedIconPath": "static/code.png",
	            "text": "我的"
	        }]
	    }
	
	
}
